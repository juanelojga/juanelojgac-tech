---
import Header from "../components/Header.astro";
import en from "../i18n/en.json";

const translations = en;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Header Example</title>
  </head>
  <body class="m-0 p-0">
    <Header
      headline={translations.header.headline}
      description={translations.header.description}
      primaryButtonText={translations.header.primaryButton}
      primaryButtonHref="/contact"
      secondaryButtonText={translations.header.secondaryButton}
      secondaryButtonHref="/about"
      videoSrc="/_videos/v1/d0e887ab38a377a5f5f7ccaf8b98086b66154fbb"
    />
  </body>
</html>
